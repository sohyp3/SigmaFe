{
  "expo": {
    "name": "Sigma Notes",
    "slug": "sigma-notes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sigmanotes",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.hyp3.sigmaNotes",
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": true
    },
    "android": {
      "package": "com.hyp3.sigmaNotes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/702d881b-72ca-48d4-af4f-a241b6eeee2a",
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-web-browser",
      "expo-updates"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "702d881b-72ca-48d4-af4f-a241b6eeee2a"
      }
    },
    "owner": "hyp3"
  }
}